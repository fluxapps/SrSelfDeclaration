{
  "name": "srag/generateplugininfoshelper",
  "description": "Automatic generate plugin infos such README.md, plugin.php, plugin.xml, ... from composer.json",
  "version": "2.10.2",
  "extra": {
    "generate_plugin_readme_template": "LIBRARY",
    "long_description_template": "src/LONG_DESCRIPTION.md"
  },
  "type": "library",
  "keywords": [
    "generate",
    "helper",
    "infos",
    "plugin"
  ],
  "homepage": "https://github.com/fluxfw/GeneratePluginInfosHelper",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\GeneratePluginInfosHelper\\SrSelfDeclaration\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "generate-plugin-readme": [
      "srag\\GeneratePluginInfosHelper\\SrSelfDeclaration\\GeneratePluginReadme::generatePluginReadme"
    ],
    "update-version": [
      "srag\\GeneratePluginInfosHelper\\SrSelfDeclaration\\UpdateVersion::updateVersion"
    ]
  }
}
